.facebook-share
  background-color #D1D9EC
  background-image url(asset_path('share-icons.png'))
  background-position -36px 0
  width 36px
  height 36px
  float left

.gplus-share
  background-color #D1D9EC
  background-image url(asset_path('share-icons.png'))
  background-position -72px 0
  margin-top 36px
  width 36px
  height 36px
  float left

.tumblr-share
  background-color #D1D9EC
  background-image url(asset_path('share-icons.png'))
  background-position -108px 0
  width 36px
  height 36px
  float left

.active-page a
  color white !important

#address-box
  background-color #F1F2F7

#address-box p
  margin 16px

.section-text
  background-color #F1F2F7
  position relative
