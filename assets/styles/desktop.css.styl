.brand-share:hover
  background url(asset_path('share-with-hover.png')) no-repeat 0 -36px

.nicescroll-rails
  width 1.875px !important
  background #E6E7E8 !important
  top 195px !important

.nicescroll-rails div
  margin-right -2px

.nicescroll-rails div:hover
  background-color #F4835E !important

.share-box
  position absolute
  top -36px
  left 180px
  width 144px
  height 72px
